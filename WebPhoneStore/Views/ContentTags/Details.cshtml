@model WebPhoneStore.Models.ContentTag

@{
    ViewBag.Title = "Chi tiết";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<h2>Chi tiết "@Model.ContentID"</h2>
<div class="modal-body">
    
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.ContentID)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ContentID)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.TagID)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TagID)
        </dd>
    </dl>
</div>
<div class="modal-footer">
    <div class="form-group">
        <div class="col-md-3">
            @Html.ActionLink("Sửa", "Edit", new { idC = Model.ContentID, idT = Model.TagID }, new { @class = "btn btn-success" })
            @Html.ActionLink("Trở về", "Index", null, new { @class = "btn btn-default" })
        </div>
    </div>
</div>
